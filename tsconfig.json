{
   "compilerOptions": {
      "target": "es2021",
      "module": "commonjs",
      "moduleResolution": "node",
      "strict": true,
      "strictNullChecks": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "noImplicitAny": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true
   }
}
