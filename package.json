{
  "name": "hop",
  "version": "1.1.11",
  "description": "An icon-first bookmark manager",
  "main": "src/public/index.tsx",
  "packageManager": "pnpm@7.5.2",
  "engines": {
    "node": ">= 16.17.1"
  },
  "scripts": {
    "start:chrome": "webpack watch --config webpack/webpack.dev.js --env target=chrome",
    "start:firefox": "concurrently --kill-others \"webpack watch --config webpack/webpack.dev.js --env target=firefox\" \"cd dist/firefox && ../../node_modules/.bin/web-ext run -p=\"../../tmp-web-ext-profile\" --profile-create-if-missing --keep-profile-changes\"",
    "build": "concurrently \"pnpm build:chrome\" \"pnpm build:firefox\"",
    "build:chrome": "webpack --config webpack/webpack.prod.js --env target=chrome",
    "build:firefox": "webpack --config webpack/webpack.prod.js --env target=firefox",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}'",
    "test": "playwright test",
    "type-check": "tsc --project src/ --noEmit && tsc --project tests/ --noEmit",
    "clean": "rm -rf dist playwright-report tmp-web-ext-profile"
  },
  "author": "Mason McElvain",
  "license": "MIT",
  "dependencies": {
    "@chakra-ui/react": "^1.6.8",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "framer-motion": "^4.1.17",
    "react": "^17.0.2",
    "react-dnd": "^14.0.2",
    "react-dnd-html5-backend": "^14.0.0",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@playwright/test": "^1.28.1",
    "@swc/core": "^1.3.23",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/webextension-polyfill": "^0.8.3",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "concurrently": "^7.1.0",
    "copy-webpack-plugin": "^8.1.0",
    "css-loader": "^5.2.0",
    "esbuild": "^0.16.10",
    "esbuild-plugin-copy": "^2.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "prettier": "^2.6.2",
    "style-loader": "^2.0.0",
    "swc-loader": "^0.2.3",
    "typescript": "^4.9.4",
    "web-ext": "^7.1.1",
    "webextension-polyfill": "^0.10.0",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0",
    "webpack-extension-manifest-plugin": "^0.7.0",
    "webpack-merge": "^5.8.0",
    "zip-webpack-plugin": "^4.0.1"
  }
}
