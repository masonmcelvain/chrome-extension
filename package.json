{
  "name": "hop",
  "version": "1.1.3",
  "description": "An icon-first bookmark manager",
  "main": "index.js",
  "scripts": {
    "start:chrome": "webpack watch --config webpack.dev.js --env target=chrome",
    "start:firefox": "concurrently --kill-others \"webpack watch --config webpack.dev.js --env target=firefox\" \"cd dist/firefox && ../../node_modules/.bin/web-ext run -p=\"../../tmp-web-ext-profile\" --profile-create-if-missing --keep-profile-changes\"",
    "build": "npm run build-chrome && npm run build-firefox",
    "build:chrome": "webpack --config webpack.prod.js --env target=chrome",
    "build:firefox": "webpack --config webpack.prod.js --env target=firefox",
    "lint": "eslint .",
    "lint:fix": "eslint --fix",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf dist tmp-web-ext-profile"
  },
  "author": "Mason McElvain",
  "license": "MIT",
  "dependencies": {
    "@chakra-ui/react": "^1.6.8",
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "axios": "^0.21.1",
    "framer-motion": "^4.1.17",
    "psl": "^1.8.0",
    "react": "^17.0.2",
    "react-dnd": "^14.0.2",
    "react-dnd-html5-backend": "^14.0.0",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "typescript": "^4.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/chrome": "^0.0.145",
    "@types/webextension-polyfill": "^0.8.3",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-loader": "^8.2.2",
    "concurrently": "^7.1.0",
    "copy-webpack-plugin": "^8.1.0",
    "css-loader": "^5.2.0",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.24.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^2.4.2",
    "prettier": "^2.6.2",
    "style-loader": "^2.0.0",
    "web-ext": "^6.8.0",
    "webextension-polyfill": "^0.9.0",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0",
    "webpack-extension-manifest-plugin": "^0.7.0",
    "webpack-merge": "^5.8.0",
    "zip-webpack-plugin": "^4.0.1"
  }
}
