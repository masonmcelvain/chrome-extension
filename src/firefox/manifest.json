{
  "name": "Hop",
  "description": "An icon-first bookmark manager",
  "version": "1.1.4",
  "manifest_version": 2,
  "browser_action": {
    "default_title": "Hop",
    "default_popup": "index.html",
    "default_icon": "icon.svg"
  },
  "icons": {
    "48": "icon.svg",
    "96": "icon.svg"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "hop@masonmcelvain.com"
    }
  }
}
